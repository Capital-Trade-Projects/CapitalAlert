generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AlertItems {
  id Int @id @default(autoincrement())
  name String
  responsavel String
  dataAprovacao Int
  tipoCobranca String
  ptax Int
  orcado Int
  realizado Int
  variacao Int
  horaOrcadas Int
  valorHora Int
  obs String
  status Boolean
  prioridade String
  automacao String
}
