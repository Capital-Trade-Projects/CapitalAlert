generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Attachment {
  id Int @id @default(autoincrement())
  anexo String
}

model AlertItems {
  id            Int    @id @default(autoincrement())
  name          String
  responsavel   String
  dataAprovacao String
  tipoCobranca  String
  ptax          String?
  orcado        String?
  realizado     String?
  variacao      String?
  horaOrcadas   String?
  valorHora     String?
  obs           String?
  status        String
  prioridade    String
}
